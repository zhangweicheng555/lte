		<activiti.version>5.22.0</activiti.version>
		<commons-io.version>2.5</commons-io.version>
		<!-- test编译不运行的方式 解决 websocket冲突的人问题 -->
		<skipTests>true</skipTests>

		<spring-cloud.version>Greenwich.RC2</spring-cloud.version>
	</properties>

	<dependencies>





